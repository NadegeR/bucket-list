{% extends 'base.html.twig' %}

{% block title %}
    {{ wish.title }} | {{ parent() }}
{% endblock %}

{% block body %}
    <h2 class="titre">{{ wish.title }} - [{{ wish.category.name}}]</h2>
    <p>{{ wish.description }}</p>
    <p>{{ wish.author }}</p>
    <p>{{wish.dateCreated|date('d/m/Y')}}</p>
{% endblock %}

